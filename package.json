{
  "name": "omni-x-api",
  "version": "1.0.0",
  "description": "Omni-X Api",
  "scripts": {
    "start": "npm run transpile && node server.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "serve": "nodemon server.js",
    "watch": "tsc -w",
    "transpile": "tsc",
    "seed": "npm run transpile && node src/db/seed.js",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "Omni",
    "Omni-X",
    "Babel",
    "Webpack Configuration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Top-Kraken/omni-x-api"
  },
  "author": "Top-Kraken",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^17.0.35",
    "@types/sequelize": "^4.28.13",
    "@types/winston": "^2.4.4",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.18.1",
    "nodemon": "^1.9.2",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "compression": "^1.6.2",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ethers": "^5.6.5",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.5.0",
    "husky": "^8.0.1",
    "joi": "^17.6.0",
    "jwt-simple": "^0.5.0",
    "mongoose": "^6.3.1",
    "moralis": "^1.7.0",
    "morgan": "^1.7.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^2.4.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.19.2",
    "web3": "^1.7.3",
    "winston": "^3.7.2"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  }
}
